import random

class Node:
    def __init__(self, data):
        self.data = data
        self.left = None
        self.center = None
        self.right = None
        
    def insertChild(self, data):
        child_type = random.choice(['left', 'center', 'right'])
        if child_type == 'left':
            self.left = Node(data)
            return self.left
        elif child_type == 'center':
            self.center = Node(data)
            return self.center
        else:
            self.right = Node(data)
            return self.right

    @staticmethod
    def treeRandomizer():
        values = list(range(10))  # Unique 0 to 9
        random.shuffle(values)
        value_index = 0

        root = Node(values[value_index])
        value_index += 1

        l2Nodes = []
        for _ in range(random.randint(1, 3)):
            if value_index >= len(values):
                break
            child = root.insertChild(values[value_index])
            l2Nodes.append(child)
            value_index += 1

        l3Nodes = []
        for parent in l2Nodes:
            for _ in range(random.randint(1, 3)):
                if value_index >= len(values):
                    break
                child = parent.insertChild(values[value_index])
                l3Nodes.append(child)
                value_index += 1

        for parent in l3Nodes:
            for _ in range(random.randint(1, 3)):
                if value_index >= len(values):
                    break
                parent.insertChild(values[value_index])
                value_index += 1

        return root

def printTree(node, level=0, prefix="Root: "):
    if node is not None:
        print(" " * (level*4) + prefix + str(node.data))
        if node.left:
            printTree(node.left, level+1, "L--- ")
        if node.center:
            printTree(node.center, level+1, "C--- ")
        if node.right:
            printTree(node.right, level+1, "R--- ")

# Example usage
randomTree = Node.treeRandomizer()
printTree(randomTree)
